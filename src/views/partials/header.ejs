<header class="header">
  <h1 class="header__logo"><a href="./">&lt;snippets&gt;</a></h1>
  <div class="header__navigation-wrapper">
    <nav class="navigation">
      <ul class="main-navigation">
        <% if (loggedIn) { %>
          <li class="main-navigation__item"><a class="main-navigation__link" href="./users/<%= sessionUserId %>">My snippets</a></li>
          <li class="main-navigation__item"><a class="main-navigation__link" href="./create">Create snippet</a></li>
        <% } %>
      </ul>
    </nav>
    <div class="header__buttons-wrapper">
      <% if (loggedIn) { %> 
        <a href="./logout"><button id="logout-button" class="btn btn--main btn--lg header__button">Log out</button></a>
      <% } else { %>
      <a href="./signup"><button id="login-button" class="btn btn--main btn--lg header__button">Sign up</button></a>
      <a href="./login"><button id="signup-button" class="btn btn--main btn--lg header__button">Log in</button></a>
      <% } %> 
    </div>
  </div>
</header>